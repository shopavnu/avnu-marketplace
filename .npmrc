# Force npm to accept consistent versions and bypass strict checks
legacy-peer-deps=true
strict-peer-dependencies=false
engine-strict=false
save-exact=false
pacakge-lock=false
strict-ssl=false
update-notifier=false
fund=false
audit=false

# Required for sqlite3 dependencies
better-sqlite3_binary=true
sqlite3_binary=true

# Override problematic versions everywhere
multer@="2.0.1"

# Force bypass of version checks during installation
fetch-retries=5
force=true
