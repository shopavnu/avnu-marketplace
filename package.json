  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "clean-build:backend": "rm -rf node_modules package-lock.json && npm install && npm run build --workspace=backend",
    "dev": "npm run dev:frontend",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run start:dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "npm run start:frontend",
    "start:frontend": "cd frontend && npm run start",
    "start:backend": "cd backend && npm run start",
    "docker:dev": "docker compose up",
    "docker:build": "docker compose build",
    "lint": "npm run lint:frontend",
    "lint:frontend": "cd frontend && npm run lint || true",
    "lint:backend": "cd backend && npm run lint || true",
    "preinstall": "npx only-allow npm",
    "postinstall": "cd backend && npm run fix-security || true",
    "pre-ci": "node ./pre-ci.js",
    "ci": "npm run pre-ci && npm ci"
  },
