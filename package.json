    "node": "^20.0.0",
    "npm": ">=10.0.0"
  },
  "resolutions": {
    "multer": "2.0.1",
    "path-to-regexp": "0.1.12",
    "qs": "6.11.0",
    "send": "0.19.0",
    "serve-static": "1.16.0",
